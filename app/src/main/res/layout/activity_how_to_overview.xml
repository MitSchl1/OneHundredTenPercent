<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HowToOverview">

    <TextView
        android:id="@+id/pushup_howtooverview"
        android:layout_width="380dp"
        android:layout_height="70dp"
        android:textSize="26sp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Liegestützte"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/pullup_howtooverview"
        android:layout_width="380dp"
        android:layout_height="70dp"
        android:textSize="26sp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Klimmzüge"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/pushup_howtooverview"/>
    <TextView
        android:id="@+id/squat_howtooverview"
        android:layout_width="380dp"
        android:layout_height="70dp"
        android:textSize="26sp"
        android:textStyle="bold"
        android:gravity="center"
        android:text="Kniebeugen"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pushup_howtooverview"/>

</androidx.constraintlayout.widget.ConstraintLayout>